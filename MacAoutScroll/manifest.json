{
    "manifest_version": 3,
    "name": "Mac AutoScroll",
    "version": "1.0.0",
    "description": "Replica of the AutoScroll feature from chrome on windows for mac and linux",
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["js/website.js"]
        }
      ],
     "web_accessible_resources": [
        {
          "resources": ["img/*.svg"],
          "matches": ["<all_urls>"]
        }
      ],
     "icons": {
        "16": "img/16.png",
        "32": "img/32.png",
        "48": "img/48.png",
        "64": "img/64.png",
        "96": "img/96.png",
        "128": "img/128.png"
     }
}